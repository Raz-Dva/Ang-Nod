{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],

    "routes": [
        {
            "src": "/",
            "dest": "./app.js"
        },
        {"src":"/*",
        "methods":[
            "POST",
            "GET",
            "PUT",
            "DELETE"
        ],
    "dest":"./app.js"}
    ]
}